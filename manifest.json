{
    "manifest_version": 3,
    "name": "ChatGPT Molly-guard",
    "version": "1.0",
    "description": "Prevents submission if specific words are typed into chat window",
    "content_scripts": [ 
        {
            "matches": ["https://chatgpt.com/*"],
            "css": ["style.css"],
            "js": ["wordList.js", "contentScript.js"]
        }
    ]
}
